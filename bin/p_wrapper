#!/usr/bin/env bash

set -e

VENV=.venv/p_wrapper

make -C $REPO_ROOT/bin venv VENV=$VENV FILE=$REPO_ROOT/hw/scripts/gen_files/p_wrapper/requirements.txt > /dev/null 2>&1

source $REPO_ROOT/bin/$VENV/bin/activate
python3.10 $REPO_ROOT/hw/scripts/gen_files/p_wrapper/gen_p_wrapper.py -t $@
deactivate
