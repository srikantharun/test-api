#!/usr/bin/env bash

set -e

VENV=.venv/gen_axe_tcl_config

make -C $REPO_ROOT/bin venv VENV=$VENV FILE=$REPO_ROOT/hw/ip/tech_cell_library/default/scripts/requirements.txt > /dev/null 2>&1

source $REPO_ROOT/bin/$VENV/bin/activate
python3.10 $REPO_ROOT/hw/ip/tech_cell_library/default/scripts/gen_axe_tcl_config.py $@
deactivate

